import Layout from "@/components/layout/layout.astro";
import Formatter from "@/components/playground/Formatter";
---

<Layout
  title="Go Formatter"
  description="Go Formatter that formats Go data, locally in your browser"
  language="go"
>
  <Formatter client:load minifier={false} language="go" />
  <Info language="go" />
  <FAQ language="go" client:idle />
</Layout>
