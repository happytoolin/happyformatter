import Info from "@/components/info/info.astro";
import Layout from "@/components/layout/layout.astro";
import Formatter from "@/components/playground/Formatter";
---

<Layout
  title="Java Formatter"
  description="Java Formatter that formats Java data, locally in your browser"
  language="java"
>
  <Formatter client:load minifier={false} language="java" />
  <Info language="java" />
  <FAQ language="java" client:idle />
</Layout>
